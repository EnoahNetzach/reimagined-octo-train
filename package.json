{
  "name": "cloud-academy-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "yarn@3.6.0",
  "scripts": {
    "dev": "yarn graphql:compile && vite",
    "build": "yarn graphql:compile && tsc && vite build",
    "format": "yarn format:file '**/*'",
    "format:file": "prettier --write --ignore-unknown",
    "graphql:compile": "graphql-codegen",
    "graphql:watch": "yarn graphql:compile -w",
    "lint": "yarn lint:file .",
    "lint:file": "./node_modules/.bin/eslint --report-unused-disable-directives --max-warnings=0",
    "postinstall": "husky install",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "jest --watch",
    "types": "yarn tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "graphql": "^16.6.0",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.11.2",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18",
    "@types/testing-library__jest-dom": "^5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.2",
    "vite": "^4.3.9"
  }
}
